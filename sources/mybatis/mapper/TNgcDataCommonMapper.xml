    <select id="countBean" resultType="java.lang.Integer" parameterType="map"  >  
    select 
    count(1)
    from t_ngc_data_common t left join t_ccs_user u on  u.f_account_number=t.account_number left join t_ngc_factory a on  a.id=u.factory
    <where>
       <if test="templateName != null and templateName != '' " >
       and ( t.template_name  LIKE CONCAT( '%', #{templateName,jdbcType=VARCHAR}, '%')
       or
       t.filenumber  LIKE CONCAT( '%', #{templateName,jdbcType=VARCHAR}, '%')
       )
      </if>
      <if test="date != null and date != '' " >
       and  t.create_time  LIKE CONCAT( '%', #{date,jdbcType=VARCHAR}, '%')
      </if>
      <if test="FactoryName != null and FactoryName != '' " >
       and ( a.name   LIKE CONCAT( '%', #{FactoryName,jdbcType=VARCHAR}, '%')
       or 
       u.username   LIKE CONCAT( '%', #{FactoryName,jdbcType=VARCHAR}, '%')
       )
      </if>
    </where>
  </select>
  
   <select id="selectTaskListByPage" resultMap="BaseResultMap"  parameterType="map" >  
    SELECT
		t.id, t.account_number, t.template_name, 
        t.create_time, t.filenumber ,t.flag1,t.flag2,t.task_id
		 from t_ngc_data_common t left join t_ccs_user u on  u.f_account_number=t.account_number left join t_ngc_factory a on  a.id=u.factory
    <where>
       <if test="templateName != null and templateName != '' " >
       and ( t.template_name  LIKE CONCAT( '%', #{templateName,jdbcType=VARCHAR}, '%')
       or
       t.filenumber  LIKE CONCAT( '%', #{templateName,jdbcType=VARCHAR}, '%')
       )
      </if>
      <if test="date != null and date != '' " >
       and  t.create_time  LIKE CONCAT( '%', #{date,jdbcType=VARCHAR}, '%')
      </if>
      <if test="FactoryName != null and FactoryName != '' " >
       and ( a.name   LIKE CONCAT( '%', #{FactoryName,jdbcType=VARCHAR}, '%')
       or 
       u.username   LIKE CONCAT( '%', #{FactoryName,jdbcType=VARCHAR}, '%')
       )
      </if>
    </where>
		 order  by  id desc
		 limit #{start,jdbcType=INTEGER},#{size,jdbcType=INTEGER}
  </select>
  
